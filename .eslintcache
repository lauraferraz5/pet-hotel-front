r\n    }\r\n\r\n    setState({\r\n      ...state,\r\n      [event.currentTarget.name]: event.currentTarget.value,\r\n    });\r\n    console.log(state);\r\n  }\r\n\r\n  async function handleFileUpload(file) {\r\n    try {\r\n      const uploadData = new FormData();\r\n\r\n      uploadData.append(\"img-upload\", file);\r\n\r\n      const response = await api.post(\"/file-upload\", uploadData);\r\n\r\n      return response.data.fileUrl;\r\n    } catch (err) {\r\n      console.error(err);\r\n    }\r\n  }\r\n\r\n  async function handleSubmit(event) {\r\n    event.preventDefault();\r\n    try {\r\n      const uploadedImageUrl = await handleFileUpload(state.picture);\r\n\r\n      const response = await api.post(\"/pet\", {\r\n        ...state,\r\n        picture: uploadedImageUrl,\r\n      });\r\n\r\n      console.log(response);\r\n\r\n      history.push(\"/dashboard\");\r\n    } catch (error) {\r\n      console.log(error.response);\r\n    }\r\n  }\r\n\r\n  return (\r\n    <form onSubmit={handleSubmit}>\r\n      <SimpleInput\r\n        name=\"name\"\r\n        label=\"Nome\"\r\n        type=\"text\"\r\n        value={state.name}\r\n        onChange={handleChange}\r\n        error={errors.name}\r\n      />\r\n      <RadioInput onChange={handleChange} />\r\n      <SelectInput\r\n        label=\"Selecionar Porte\"\r\n        id=\"size\"\r\n        name=\"size\"\r\n        options={size}\r\n        value={state.size}\r\n        onChange={handleChange}\r\n      />\r\n      <SelectInput\r\n        label=\"Selecionar Gênero\"\r\n        id=\"breed\"\r\n        name=\"breed\"\r\n        options={breed}\r\n        value={state.breed}\r\n        onChange={handleChange}\r\n      />\r\n      <SimpleInput\r\n        name=\"recomendations\"\r\n        label=\"Recomendações\"\r\n        type=\"text\"\r\n        value={state.recomendations}\r\n        onChange={handleChange}\r\n      />\r\n      <InputTags\r\n        onTag={getAllergyTags}\r\n        tagColor=\"#fc9c60\"\r\n        placeHolder=\"Alergias\"\r\n      />\r\n      <InputTags\r\n        onTag={getDiseaseTags}\r\n        tagColor=\"#fc9c60\"\r\n        placeHolder=\"Doenças\"\r\n      />\r\n      <FileInput\r\n        id=\"picture\"\r\n        name=\"picture\"\r\n        label=\"Enviar Imagem\"\r\n        value={state.picture}\r\n        onChange={handleChange}\r\n      />\r\n      <div className=\"botoes-cadastro\">\r\n        <button className=\"botao-azul\" type=\"button\" onClick={goBack}>\r\n          Voltar\r\n        </button>\r\n        <button className=\"botao-laranja\" type=\"button\" onClick={handleSubmit}>\r\n          Ok\r\n        </button>\r\n      </div>\r\n    </form>\r\n  );\r\n}\r\n\r\nexport default NewPet;\r\n","/mnt/c/pet-hotel-front/src/components/form/SimpleInput.js",[],"/mnt/c/pet-hotel-front/src/components/form/Login.js",[],"/mnt/c/pet-hotel-front/src/components/form/SelectInput.js",[],"/mnt/c/pet-hotel-front/src/components/form/FileInput.js",[],"/mnt/c/pet-hotel-front/src/components/form/RadioInput.js",[],"/mnt/c/pet-hotel-front/src/api/HotelApi.js",[],"/mnt/c/pet-hotel-front/src/components/cadastro/Cadastro.js",[],"/mnt/c/pet-hotel-front/src/components/form/FormCadastro.js",[],{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","severity":1,"message":"124","line":1,"column":27,"nodeType":"125","messageId":"126","endLine":1,"endColumn":36},{"ruleId":"123","severity":1,"message":"127","line":8,"column":7,"nodeType":"125","messageId":"126","endLine":8,"endColumn":19},{"ruleId":"123","severity":1,"message":"128","line":47,"column":18,"nodeType":"125","messageId":"126","endLine":47,"endColumn":27},{"ruleId":"129","severity":1,"message":"130","line":68,"column":7,"nodeType":"131","messageId":"132","endLine":68,"endColumn":26},"no-native-reassign",["133"],"no-negated-in-lhs",["134"],"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'loggedInUser' is assigned a value but never used.","'setErrors' is