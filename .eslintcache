l: null,\r\n  });\r\n\r\n  const history = useHistory();\r\n\r\n  const size = [\"Big\", \"Medium\", \"Small\"];\r\n\r\n  const breed = [\"Macho\", \"Fêmea\"];\r\n\r\n  function goBack() {\r\n    history.push(\"/dashboard\");\r\n  }\r\n\r\n  function handleChange(event) {\r\n    if (event.currentTarget.files) {\r\n      return setState({\r\n        ...state,\r\n        [event.currentTarget.name]: event.currentTarget.files[0],\r\n      });\r\n      console.log(state);\r\n    }\r\n\r\n    setState({\r\n      ...state,\r\n      [event.currentTarget.name]: event.currentTarget.value,\r\n    });\r\n    console.log(state);\r\n  }\r\n\r\n  async function handleFileUpload(file) {\r\n    try {\r\n      const uploadData = new FormData();\r\n\r\n      uploadData.append(\"img-upload\", file);\r\n\r\n      const response = await api.post(\"/file-upload\", uploadData);\r\n\r\n      return response.data.fileUrl;\r\n    } catch (err) {\r\n      console.error(err);\r\n    }\r\n  }\r\n\r\n  async function handleSubmit(event) {\r\n    event.preventDefault();\r\n    try {\r\n      const uploadedImageUrl = await handleFileUpload(state.picture);\r\n\r\n      const response = await api.post(\"/pet\", {\r\n        ...state,\r\n        picture: uploadedImageUrl,\r\n      });\r\n\r\n      console.log(response);\r\n\r\n      history.push(\"/dashboard\");\r\n    } catch (error) {\r\n      console.log(error.response);\r\n    }\r\n  }\r\n\r\n  return (\r\n    <form onSubmit={handleSubmit}>\r\n      <SimpleInput\r\n        name=\"name\"\r\n        label=\"Nome\"\r\n        type=\"text\"\r\n        value={state.name}\r\n        onChange={handleChange}\r\n        error={errors.name}\r\n      />\r\n      <RadioInput onChange={handleChange} />\r\n      <SelectInput\r\n        label=\"Selecionar Porte\"\r\n        id=\"size\"\r\n        name=\"size\"\r\n        options={size}\r\n        value={state.size}\r\n        onChange={handleChange}\r\n      />\r\n      <SelectInput\r\n        label=\"Selecionar Gênero\"\r\n        id=\"breed\"\r\n        name=\"breed\"\r\n        options={breed}\r\n        value={state.breed}\r\n        onChange={handleChange}\r\n      />\r\n      <SimpleInput\r\n        name=\"recomendations\"\r\n        label=\"Recomendações\"\r\n        type=\"text\"\r\n        value={state.recomendations}\r\n        onChange={handleChange}\r\n      />\r\n      <InputTags\r\n        onTag={getAllergyTags}\r\n        tagColor=\"#fc9c60\"\r\n        placeHolder=\"Alergias\"\r\n      />\r\n      <InputTags\r\n        onTag={getDiseaseTags}\r\n        tagColor=\"#fc9c60\"\r\n        placeHolder=\"Doenças\"\r\n      />\r\n      <FileInput\r\n        id=\"picture\"\r\n        name=\"picture\"\r\n        label=\"Enviar Imagem\"\r\n        value={state.picture}\r\n        onChange={handleChange}\r\n      />\r\n      <div className=\"botoes-cadastro\">\r\n        <button className=\"botao-azul\" type=\"button\" onClick={goBack}>\r\n          Voltar\r\n        </button>\r\n        <button className=\"botao-laranja\" type=\"button\" onClick={handleSubmit}>\r\n          Ok\r\n        </button>\r\n      </div>\r\n    </form>\r\n  );\r\n}\r\n\r\nexport default NewPet;\r\n","/mnt/c/pet-hotel-front/src/components/form/SimpleInput.js",[],"/mnt/c/pet-hotel-front/src/components/form/Login.js",[],"/mnt/c/pet-hotel-front/src/components/form/SelectInput.js",[],"/mnt/c/pet-hotel-front/src/components/form/FileInput.js",[],"/mnt/c/pet-hotel-front/src/components/form/RadioInput.js",[],"/mnt/c/pet-hotel-front/src/api/HotelApi.js",[],"/mnt/c/pet-hotel-front/src/components/cadastro/Cadastro.js",[],"/mnt/c/pet-hotel-front/src/components/form/FormCadastro.js",[],"/mnt/c/pet-hotel-front/src/components/form/FormPet.js",[],{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","severity":1,"message":"127","line":9,"column":7,"nodeType":"128","messageId":"129","endLine":9,"endColumn":19},{"ruleId":"130","severity":1,"message":"131","line":34,"column":5,"