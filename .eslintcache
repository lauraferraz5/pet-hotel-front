ory = useHistory();\r\n\r\n  function goBack() {\r\n    history.push(\"/dashboard\");\r\n  }\r\n\r\n  function deletar() {\r\n    api.delete(`/pet/${id}`);\r\n    props.history.push(\"/dashboard\");\r\n  }\r\n\r\n  return (\r\n    <div className=\"container\">\r\n      <div className=\"cadastro2\">\r\n        <h2>Editar Pet</h2>\r\n        <FormPet\r\n          state={state}\r\n          handleChange={handleChange}\r\n          handleSubmit={handleSubmit}\r\n          onSubmit={handleSubmit}\r\n          getAllergyTags={getAllergyTags}\r\n          getDiseaseTags={getDiseaseTags}\r\n          goBack={goBack}\r\n          errors={errors}\r\n        />\r\n        <div className=\"delete-btn-div\">\r\n          <button className=\"botao-vermelho\" type=\"button\" onClick={deletar}>\r\n            Deletar\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default EditPet;\r\n","/mnt/c/pet-hotel-front/src/components/booking/PetPicker.js",["144"],"/mnt/c/pet-hotel-front/src/components/form/NovoCadastro.js",[],{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","severity":1,"message":"150","line":9,"column":7,"nodeType":"151","messageId":"152","endLine":9,"endColumn":19},{"ruleId":"153","severity":1,"message":"154","line":34,"column":5,"nodeType":"155","endLine":34,"endColumn":7,"suggestions":"156"},{"ruleId":"157","severity":1,"message":"158","line":150,"column":15,"nodeType":"159","endLine":150,"endColumn":30},{"ruleId":"157","severity":1,"message":"158","line":157,"column":15,"nodeType":"159","endLine":157,"endColumn":30},{"ruleId":"157","severity":1,"message":"158","line":164,"column":15,"nodeType":"159","endLine":164,"endColumn":30},{"ruleId":"149","severity":1,"message":"160","line":36,"column":13,"nodeType":"151","messageId":"152","endLine":36,"endColumn":21},{"ruleId":"149","severity":1,"message":"161","line":26,"column":18,"nodeType":"151","messageId":"152","endLine":26,"endColumn":